<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <h2>Albums</h2>
    <table>
      <tr>
        <th>AlbumID</th>
        <th>AlbumName</th>
        <th>CoverImg</th>
        <th>CreateDate</th>
        <th>Creater</th>
      </tr>
      <% for ( var i = 0 ; i < data.length ; i++){ %>
         <tr>
           <td>
             <%= data[i].AlbumID  %>
           </td>
           <td>
	     <a href= "<%= data[i].URL %>"><%= data[i].AlbumName  %></a>
           </td>
           <td>
             <%= data[i].CoverImg  %>
           </td>
           <td>
             <%= data[i].CreateDate  %>
           </td>
           <td>
             <%= data[i].Creater  %>
           </td>
         </tr>
         <% } %>
    </table>
    <script src="/javascripts/index.vue.js"></script>
  </body>
</html>
