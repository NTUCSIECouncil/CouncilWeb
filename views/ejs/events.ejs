<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <h2>Events</h2>
    <table>
      <tr>
        <th>EventID</th>
        <th>EventName</th>
        <th>Priority</th>
        <th>CreateDate</th>
        <th>Content</th>
      </tr>
      <% for ( var i = 0 ; i < data.length ; i++){ %>
         <tr>
           <td>
             <%= data[i].EventID  %>
           </td>
           <td>
             <%= data[i].EventName  %>
           </td>
           <td>
             <%= data[i].CreateDate  %>
           </td>
           <td>
             <%= data[i].Content  %>
           </td>
         </tr>
         <% } %>
    </table>
    <script src="/javascripts/index.vue.js"></script>
  </body>
</html>
